{
  "name": "dive-dsa",
  "version": "1.11.17",
  "author": {
    "name": "Kitware, Inc.",
    "email": "Bryon.Lewis@kitware.com"
  },
  "description": "DIVE annotation platform for Digital Slide Archive",
  "homepage": "https://digitalslidearchive.github.io/dive-dsa/",
  "license": "Apache-2.0",
  "scripts": {
    "serve": "vue-cli-service serve platform/web-girder/main.ts",
    "build:web": "vue-cli-service build platform/web-girder/main.ts",
    "build:package": "vue-cli-service build --target lib --name DIVE-DSA platform/web-girder/main.ts",
    "build:lib": "rollup -c",
    "build:cli": "tsc -b tsconfig.cli.json && tsc-alias -p tsconfig.cli.json",
    "lint": "vue-cli-service lint src/ dive-common/ platform/",
    "lint:templates": "vtc --workspace . --srcDir src/",
    "test": "vue-cli-service test:unit src/ dive-common/ platform/"
  },
  "resolutions": {},
  "files": [
    "/dist"
  ],
  "main": "lib/index.js",
  "types": "lib/types/index.d.ts",
  "dependencies": {
    "@flatten-js/interval-tree": "^1.0.11",
    "@girder/components": "^3.2.0",
    "@mdi/font": "^6.2.95",
    "@mdi/js": "^7.4.47",
    "axios": "^1.6.7",
    "color-hash": "^1.0.3",
    "core-js": "^3.22.2",
    "csv-stringify": "^5.6.0",
    "d3": "^5.12.0",
    "geojs": "^1.15.1",
    "glob-to-regexp": "^0.4.1",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.19",
    "moment": "^2.29.1",
    "mousetrap": "^1.6.5",
    "semver": "^7.3.5",
    "vue": "^2.7.15",
    "vue-girder-slicer-cli-ui": "^0.1.3",
    "vue-gtag": "^1.9.1",
    "vue-router": "^3.0.3",
    "vuetify": "^2.3.10",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.19.0",
    "@types/color-hash": "^1.0.0",
    "@types/cors": "^2.8.9",
    "@types/csv-parse": "^1.2.2",
    "@types/d3": "^5.7.2",
    "@types/express": "^4.17.9",
    "@types/geojson": "^7946.0.7",
    "@types/glob-to-regexp": "^0.4.0",
    "@types/jest": "^27.0.4",
    "@types/js-cookie": "^3.0.6",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash": "4.17.0",
    "@types/mime-types": "^2.1.0",
    "@types/mock-fs": "^4.13.1",
    "@types/mousetrap": "^1.6.3",
    "@types/node": "^12.18.3",
    "@types/proper-lockfile": "^4.1.1",
    "@types/pump": "^1.1.0",
    "@types/range-parser": "^1.2.3",
    "@types/request": "^2.48.5",
    "@types/resize-observer-browser": "^0.1.4",
    "@types/semver": "^7.3.8",
    "@types/source-map": "0.5.2",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "@vue/cli-plugin-babel": "~5.0.8",
    "@vue/cli-plugin-eslint": "~5.0.8",
    "@vue/cli-plugin-typescript": "~5.0.8",
    "@vue/cli-plugin-unit-jest": "~5.0.8",
    "@vue/cli-service": "~5.0.8",
    "@vue/compiler-sfc": "^3.2.20",
    "@vue/eslint-config-airbnb": "^7.0.1",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.5.1",
    "babel-register": "^6.26.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "csv-parse": "^4.13.1",
    "eslint": "^8.55.0",
    "eslint-import-resolver-typescript": "^2.2.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-vue": "^9.19.2",
    "express": "^4.17.1",
    "fs-extra": "^9.0.1",
    "git-describe": "^4.0.4",
    "jest": "^27.0.4",
    "jest-transform-stub": "^2.0.0",
    "js-yaml": "^4.1.0",
    "mime-types": "^2.1.27",
    "mock-fs": "^5.1.1",
    "proper-lockfile": "^4.1.1",
    "pump": "^3.0.0",
    "range-parser": "^1.2.1",
    "request": "^2.88.2",
    "rollup": "^3.29.5",
    "rollup-plugin-cleaner": "^1.0.0",
    "rollup-plugin-scss": "^3.0.0",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "~1.32.6",
    "sass-loader": "^8.0.2",
    "tailwindcss": "^3.3.6",
    "ts-jest": "^27.0.4",
    "tsc-alias": "^1.2.0",
    "tslib": "^2.6.2",
    "typescript": "~4.3.5",
    "vue-cli-plugin-vuetify": "^2.4.3",
    "vue-jest": "^3.0.7",
    "vue-template-compiler": "^2.7.0",
    "vuetify-loader": "^1.7.3",
    "worker-loader": "^3.0.8",
    "xml-js": "^1.6.11",
    "yargs": "^16.1.0"
  },
  "jest": {
    "verbose": true,
    "moduleNameMapper": {
      "^vue-media-annotator/(.*)$": "<rootDir>/src/$1",
      "^dive-common/(.*)$": "<rootDir>/dive-common/$1",
      "^platform/(.*)$": "<rootDir>/platform/$1",
      "\\.css$": "<rootDir>/test/stub.js"
    },
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.jsx?$": "babel-jest"
    },
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.spec.json"
      }
    },
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:vue/recommended",
      "@vue/airbnb"
    ],
    "rules": {
      "no-underscore-dangle": 0,
      "spaced-comment": "off",
      "no-useless-constructor": "off",
      "no-use-before-define": "off",
      "vue/require-valid-default-prop": "off",
      "vue/require-default-prop": "off",
      "vue/no-unused-components": "off",
      "vue/valid-v-slot": "off",
      "vuejs-accessibility/alt-text": "off",
      "vue/no-v-text-v-html-on-component": "off",
      "@typescript-eslint/no-unused-vars": "off",
      "vue/no-template-target-blank": "off",
      "vue/no-lone-template": "off",
      "default-param-last": "off",
      "vuejs-accessibility/mouse-events-have-key-events": "off",
      "vuejs-accessibility/click-events-have-key-events": "off",
      "vue/no-mutating-props": "off",
      "no-promise-executor-return": "off",
      "no-shadow": "off",
      "vue/multi-word-component-names": "off",
      "vuejs-accessibility/form-control-has-label": "off",
      "@typescript-eslint/ban-ts-comment": "off",
      "@typescript-eslint/ban-ts-ignore": "off",
      "@typescript-eslint/no-useless-constructor": "error",
      "import/extensions": "off",
      "vue/max-len": "off",
      "no-console": [
        "warn",
        {
          "allow": [
            "warn",
            "error"
          ]
        }
      ],
      "semi": "off",
      "@typescript-eslint/semi": [
        "error"
      ]
    },
    "settings": {
      "import/resolver": {
        "typescript": {}
      }
    },
    "overrides": [
      {
        "files": [
          "**/*.ts",
          "**/*.vue"
        ],
        "extends": [
          "@vue/typescript",
          "@vue/typescript/recommended"
        ]
      }
    ],
    "parserOptions": {
      "parser": "@typescript-eslint/parser",
      "ecmaVersion": 2020
    }
  }
}
